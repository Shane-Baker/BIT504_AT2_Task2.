public class Cell {
    // Content of this cell (empty, cross, nought)
    Player content;
    // Row and column of this cell
    int row, col;
    
    /** Constructor to initialise this cell with the specified row and col */
    public Cell(int row, int col) {
        this.row = row;
        this.col = col;
        // Set the cell content to EMPTY initially
        clear();
    }
    

}
